{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@features/*": ["src/features/*"],
			"@core/*": ["src/core/*"],
			"@assets/*": ["src/assets/*"],
			"@content/*": ["src/content/*"]
		}
	}
}
